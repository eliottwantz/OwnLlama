{
  "name": "rag-qdrant",
  "type": "module",
  "scripts": {
    "start": "bun run main.ts",
    "dev": "bun run --watch main.ts",
    "qdrant": "docker run --rm -d --name rag-qdrant -p 6333:6333 -p 6334:6334 -v $(pwd)/qdrant_storage:/qdrant/storage qdrant/qdrant",
    "qdrant:ui": "xdg-open http://localhost:6333/dashboard",
    "typecheck": "tsc"
  },
  "devDependencies": {
    "@types/bun": "1",
    "typescript": "5"
  },
  "dependencies": {
    "@langchain/nomic": "^0.0.4",
    "@qdrant/js-client-rest": "^1.9.0",
    "elysia": "1",
    "langchain": "^0.1.36",
    "ollama": "^0.5.0"
  },
  "overrides": {
    "@langchain/core": "0.1.36"
  }
}
